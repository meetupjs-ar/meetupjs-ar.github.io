{
  "env": {
    "NODE_ENV": "production"
  },
  "files": ["build", "serve.json"],
  "public": true,
  "scale": {
    "sfo1": {
      "min": 1,
      "max": 1
    }
  },
  "static": {
    "headers": [
      {
        "source": "/static/**/*.*",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=86400"
          }
        ]
      }
    ],
    "trailingSlash": true
  },
  "type": "npm"
}
